{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver", {
        "extensions": [".ts", ".tsx", ".js", ".jsx"],
        "root": ["."],
        "alias": {
          "@": "./src"
        }
      }
    ]
  ]
}